        <spring.data.jpa.version>2.0.3.RELEASE</spring.data.jpa.version>
        <shiro.version>1.3.2</shiro.version>
    </properties>

    <repositories>
        <repository>
            <id>aliyunmaven</id>
            <url>http://maven.aliyun.com/nexus/content/groups/public/</url>
        </repository>
    </repositories>

    <modules>
        <module>core</module>
        <module>common-jts</module>
        <module>account</module>
        <module>account-common</module>
        <module>account-web</module>
        <module>common-log4j</module>
        <module>common-upload</module>
        <module>work-system</module>
